{
  "name": "nextxp-demo",
  "version": "1.0.2",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "dev": "npm run server-build && cross-env NODE_ENV=development node dist/server/server.js",
    "start": "cross-env NODE_ENV=production node dist/server/server.js",
    "prod": "npm run build && npm run start",
    "build": "npm run client-build && npm run server-build",
    "client-build": "npx next build",
    "server-build": "tsc --build ./server/tsconfig.json",
    "lint": "npx next lint"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "html-react-parser": "^4.0.0",
    "next": "13.4.7",
    "next-transpile-modules": "^10.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "unescape": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.4.1",
    "@types/react": "18.2.16",
    "@types/react-dom": "^18.2.6",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.10",
    "prettier": "^3.0.0",
    "typescript": "5.1.6"
  }
}
